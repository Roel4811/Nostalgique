<%= form_for :member, url: '/members' do |f| %>

<br>
<div class="form-group">
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
</div>

<div class="form-group">
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
</div>

<div class="form-group">
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <br>
</div>

<div class="form-group">
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
</div>

<div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
</div>

  <%= f.submit "Submit" %>

<% end %>
